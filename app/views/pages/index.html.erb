<script>
	let buttonSub = document.getElementById("btn2");
	let text = document.getElementById("red-1");

	let checkbox1 = document.getElementById("checkbox1");
	let checkbox2 = document.getElementById("checkbox2");
	let checkbox7 = document.getElementById("checkbox7");
	let checkbox8 = document.getElementById("checkbox8");

	buttonSub.disabled = true;
	buttonSub.style.background = "gray";

	$("div").click(function(){
		if (checkbox1.checked) {
			buttonSub.disabled = false;
			buttonSub.style.background = "#f56b3b";
			text.style.display = "none";
		} else if (checkbox2.checked) {
			buttonSub.disabled = false;
			buttonSub.style.background = "#f56b3b";
			text.style.display = "none";
		} else if (checkbox7.checked) {
			buttonSub.disabled = false;
			buttonSub.style.background = "#f56b3b";
			text.style.display = "none";
		} else if (checkbox8.checked) {
			buttonSub.disabled = false;
			buttonSub.style.background = "#f56b3b";
			text.style.display = "none";
		} else {
			buttonSub.disabled = true;
			buttonSub.style.background = "gray";
			text.style.display = "block";
		}
	});

	var changed = function () {

		Array.from(document.querySelectorAll("table")).forEach((element, index) => {
			Array.from(element.querySelectorAll("input")).forEach((myinp, index) => {

				if (myinp.checked == true) {
					myinp.parentNode.parentNode.classList.add("active");
				} else {
					myinp.parentNode.parentNode.classList.remove("active");
				}

			});
		});
	} //changed function ends

	Array.from(document.querySelectorAll("table")).forEach((element, index) => {
		Array.from(element.querySelectorAll("input")).forEach((myinp, index) => {
			var checkmark = document.createElement("span");
			checkmark.classList.add("checkmark");
			myinp.parentNode.appendChild(checkmark);
			myinp.addEventListener("change", changed);
		});
	});


	$(function () {
		document.getElementById("submit_form").window.Parsley.on('field:validated', function () {
				 var ok = $('.parsley-error').length === 0;
				 $('.bs-callout-info').toggleClass('hidden', !ok);
				 $('.bs-callout-warning').toggleClass('hidden', ok);
			 })
			 .on('form:submit', function () {
				 return false; // Don't submit form for this demo
			 });
	 });
</script>

